---
import JellyfinApi from "../model/jellyfinapi";
import UserManager from "../model/usermanager";
import { Jellyfin, Api } from '@jellyfin/sdk';
import { UserApi } from '@jellyfin/sdk/lib/generated-client/api'
import getConfig from "../model/config";
import { Configuration } from "@jellyfin/sdk/lib/generated-client/configuration";

const userApi = new UserApi(new Configuration({
    apiKey: getConfig().jellyfin.token,
    basePath: getConfig().jellyfin.url,
}));

const result = (await userApi.authenticateUser({userId: 'gaspa', pw: 'Gaspi2003'})).data.AccessToken;
---
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <h1>{{ result }}</h1>
</body>
</html>